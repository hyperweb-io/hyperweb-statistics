    "url": "https://github.com/cosmology/db-utils"
  },
  "bugs": {
    "url": "https://github.com/cosmology/db-utils/issues"
  },
  "scripts": {
    "copy": "copyfiles -f ../../LICENSE README.md package.json dist",
