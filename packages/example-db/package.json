{
  "name": "@hyperweb-io/example-db",
  "version": "0.0.1",
  "author": "Hyperweb <developers@hyperweb.io>",
  "description": "example",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "homepage": "https://github.com/hyperweb-io/db-utils",
  "license": "SEE LICENSE IN LICENSE",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperweb-io/db-utils"
  },
  "bugs": {
    "url": "https://github.com/hyperweb-io/db-utils/issues"
  },
  "scripts": {
    "copy": "copyfiles -f ../../LICENSE README.md package.json dist",
    "clean": "rimraf dist/**",
    "prepare": "npm run build",
    "build": "npm run clean; tsc; tsc -p tsconfig.esm.json; npm run copy",
    "build:dev": "npm run clean; tsc --declarationMap; tsc -p tsconfig.esm.json; npm run copy",
    "dev": "DATABASE_URL=postgres://postgres:password@localhost:5432/example_db ts-node ./src/dev.ts",
    "npm:fetch:packages": "DATABASE_URL=postgres://postgres:password@localhost:5432/example_db ts-node ./src/tasks/npm/npm.tasks.ts fetch:packages",
    "npm:fetch:downloads": "DATABASE_URL=postgres://postgres:password@localhost:5432/example_db ts-node ./src/tasks/npm/npm.tasks.ts fetch:downloads",
    "npm:fetch:downloads:reset": "DATABASE_URL=postgres://postgres:password@localhost:5432/example_db ts-node ./src/tasks/npm/npm.tasks.ts fetch:downloads:reset",
    "npm:report": "DATABASE_URL=postgres://postgres:password@localhost:5432/example_db ts-node ./src/tasks/npm/npm.tasks.ts generate:report",
    "gh:fetch": "ts-node src/tasks/github/github.tasks.ts fetch",
    "gh:report": "ts-node src/tasks/github/github.tasks.ts report",
    "gh:export": "ts-node src/tasks/github/github.tasks.ts export",
    "gh:analyze": "ts-node src/tasks/github/analyze-repo.ts",
    "db:dump": "DATABASE_URL=postgres://postgres:password@localhost:5432/example_db ts-node ./src/dump.ts",
    "fetch": "ts-node ./src/fetch.ts",
    "lint": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@cosmology/db-client": "^0.1.0",
    "@interweb/fetch-api-client": "^0.6.0",
    "@interweb/http-errors": "^0.1.0",
    "@octokit/graphql": "^8.2.1",
    "@octokit/rest": "^21.1.1",
    "@types/pg": "^8.11.10",
    "dotenv": "^16.4.7",
    "pg": "^8.13.1"
  },
  "keywords": []
}